# Add API keys for connecting to LLM providers, data sources, and other integrations here
GOOGLE_API_KEY=...
GOOGLE_MODEL_NAME=...
GOOGLE_APPLICATION_CREDENTIALS=...

OPENAI_API_KEY=...

ANTHROPIC_API_KEY=...


# Database Configuration
DATABASE_URI=postgresql://langgraph:langgraph@postgres:5432/langgraph
REDIS_URI=redis://redis:6379

# Runtime Configuration
LANGGRAPH_RUNTIME_EDITION=postgres
LANGGRAPH_AES_KEY=12345678901234567890123456789012

# Worker Configuration
N_JOBS_PER_WORKER=10
BG_JOB_ISOLATED_LOOPS=true
BG_JOB_TIMEOUT_SECS=3600

# Authentication Configuration
LANGGRAPH_AUTH_TYPE=noop  # Options: noop, langsmith, custom
LANGSMITH_API_KEY=your-langsmith-api-key
LANGSMITH_TENANT_ID=your-tenant-id

# CORS Configuration
CORS_ALLOW_ORIGINS=*

# Logging Configuration
LANGGRAPH_SERVER_LEVEL=INFO

# Optional: Store Configuration
LANGGRAPH_STORE={"path": "/app/store", "index": {"dims": 1536, "embed": "openai:text-embedding-3-small"}}

# Optional: HTTP Configuration
LANGGRAPH_HTTP={"disable_ui": false, "disable_mcp": true}

# Optional: Thread TTL Configuration
LANGGRAPH_THREAD_TTL={"strategy": "delete", "default_ttl": 1440, "sweep_interval_minutes": 60}

# Optional: Checkpointer Configuration
LANGGRAPH_CHECKPOINTER={"ttl": {"strategy": "delete", "default_ttl": 1440, "sweep_interval_minutes": 60}}

# Optional: Mount Prefix
MOUNT_PREFIX=/api/v1

# Optional: LangSmith Tracing
LANGCHAIN_TRACING_V2=true
LANGSMITH_TRACING_V2=true