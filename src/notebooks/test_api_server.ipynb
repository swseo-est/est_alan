    "   \"outputs\": [],\n",
    "   \"source\": [\n",
    "    \"def get_data(chunk):\\n\",\n",
    "    \"    data = list(chunk.data.values())[0]\\n\",\n",
    "    \"    return data\\n\",\n",
    "    \" \\n\",\n",
    "    \" \\n\",\n",
    "    \"def filter_ai_msg(chunk):\\n\",\n",
